MEMORY {
    RAM     (rw)    : ORIGIN = 0x0, LENGTH = 128k
    STACK   (rw)    : ORIGIN = 0xfc00, LENGTH = 768
}

SECTIONS {
    .text : {
        . = 0x00000000;
        * (VECTOR_TABLE)    /* asm */
        . = 0x00001000;
        * (CODE)
        . = 0x00002000;
        * (DICT)
        . = 0x00003000;
        * (BUFFER)
    } > RAM
    .data : {} > RAM
    .bss  : {} > RAM
    .stack : {} > STACK
}